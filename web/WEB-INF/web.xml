<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>servletOrigen</servlet-name>
        <servlet-class>Controlador.servletOrigen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletDestino</servlet-name>
        <servlet-class>Controlador.servletDestino</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletOrigenDestino</servlet-name>
        <servlet-class>Controlador.servletOrigenDestino</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletServicios</servlet-name>
        <servlet-class>Controlador.servletServicios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletOcupacion</servlet-name>
        <servlet-class>Controlador.servletOcupacion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletOrigen</servlet-name>
        <url-pattern>/servletOrigen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletDestino</servlet-name>
        <url-pattern>/servletDestino</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletOrigenDestino</servlet-name>
        <url-pattern>/servletOrigenDestino</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletServicios</servlet-name>
        <url-pattern>/servletServicios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletOcupacion</servlet-name>
        <url-pattern>/servletOcupacion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
